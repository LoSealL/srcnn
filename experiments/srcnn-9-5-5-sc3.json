{
  "save_dir": "srcnn-9-5-5-sc3",
  "model": {
    "name": "srcnn",
    "params": {"f": [9, 5, 5],  "n": [64, 32]}
  },
  "loss":{
    "name": "mse_pl_tv",
    "weight0": 1,
    "weight1": 4,
    "weight2": 0.001,
    "block": 3,
    "conv": 2
  },
  "pre_upsample": true,
  "scale": 3,
  "train_set": "91-image",
  "val_set": "Set5",
  "test_sets": ["Set5", "Set14"],
  "lr_sub_size": 11,
  "lr_sub_stride": 5,
  "epochs": 200,
  "optimizer": {
    "name": "adam",
    "params": {
      "lr": 1e-5,
      "decay": 1e-5
    }
  }
}
